@using EntityLayer.Concrete
@model List<Experience>

<div class="card">
    <div class="card-header">
        <h3 class="mb-0">Deneyimlerim</h3>
    </div>
    <div class="card-body">
        @foreach(var item in Model){
        <div class="mb-4">
            <h5>@item.Name</h5>
            <p class="text-muted">@item.Date</p>
            <ul>
                <li>@item.Description</li>

            </ul>
        </div>
        <hr />
        }
    </div>
</div>